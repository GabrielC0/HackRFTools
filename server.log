order: none;\n        color: white;\n        font-size: 1.2em;\n        cursor: pointer;\n        z-index: 100;\n        width: 40px;\n        height: 40px;\n        border-radius: 50%;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        background: rgba(255, 255, 255, 0.1);\n        transition: all 0.3s ease;\n      }\n\n      .refresh-btn:hover {\n        background: rgba(255, 255, 255, 0.2);\n      }\n\n      .refresh-btn.spinning i {\n        animation: spin 1s linear infinite;\n      }\n\n      @media (max-height: 480px) {\n        .virtual-keyboard {\n          max-height: 22vh;\n          padding: 0.5px;\n          gap: 1px;\n        }\n        .key {\n          font-size: 1em;\n          min-width: 24px;\n          min-height: 24px;\n        }\n      }\n\n      .current-network {\n        background-color: rgba(0, 255, 0, 0.1);\n        border-left: 3px solid #00ff00;\n      }\n\n      .connection-status {\n        color: #00ff00;\n        font-size: 0.8em;\n        margin-left: 10px;\n      }\n    </style>\n  </head>\n  <body>\n    <div class="container">\n      <button class="back-button" onclick="window.location.href=\'/\'">\n        <i class="fas fa-arrow-left"></i>\n      </button>\n\n      <button class="refresh-btn" onclick="handleRefresh()">\n        <i class="fas fa-sync-alt"></i>\n      </button>\n\n      <div class="header">\n        <h1 class="title">Configuration WiFi</h1>\n        <p class="subtitle">Configurez la connexion WiFi de votre appareil</p>\n      </div>\n\n      <div class="wifi-status">\n        <div class="status-icon">\n          <i class="fas fa-wifi"></i>\n        </div>\n        <div class="status-text">Chargement...</div>\n        <div class="ip-info">\n          <i\n            class="fas fa-network-wired"\n            style="margin-right: 5px; opacity: 0.7"\n          ></i>\n          <span id="ipAddress">Chargement...</span>\n        </div>\n      </div>\n\n      <div class="wifi-list" id="wifiList"></div>\n\n      <div class="toast"></div>\n    </div>\n\n    <script>\n      let selectedNetwork = null;\n      const wifiList = document.getElementById("wifiList");\n      const toast = document.querySelector(".toast");\n      const statusText = document.querySelector(".status-text");\n      const statusIcon = document.querySelector(".status-icon i");\n\n      // Fonction pour afficher un message toast\n      function showToast(message, type = "") {\n        toast.textContent = message;\n        toast.className = "toast" + (type ? " " + type : "");\n        toast.style.display = "block";\n        setTimeout(() => {\n          toast.style.display = "none";\n        }, 3000);\n      }\n\n      // Fonction pour mettre \xc3\xa0 jour le statut de connexion\n      function updateConnectionStatus() {\n        fetch("/wifi-status")\n          .then((response) => response.json())\n          .then((data) => {\n            if (data.connected) {\n              statusText.textContent = "Connect\xc3\xa9 \xc3\xa0 " + data.ssid;\n              statusIcon.className = "fas fa-wifi connected-icon";\n            } else {\n              statusText.textContent = "Non connect\xc3\xa9";\n              statusIcon.className = "fas fa-wifi-slash disconnected-icon";\n            }\n          })\n          .catch((error) => {\n            console.error("Erreur:", error);\n            statusText.textContent = "Erreur de connexion";\n            statusIcon.className = "fas fa-exclamation-circle";\n          });\n      }\n\n      // Fonction pour r\xc3\xa9cup\xc3\xa9rer l\'adresse IP\n      function getIpAddress() {\n        fetch("/get-ip")\n          .then((res) => res.json())\n          .then((data) => {\n            if (data.status === "success") {\n              const ipInfo = data.ip_info;\n              let ipDisplay = "";\n\n              if (ipInfo.wlan0) {\n                ipDisplay = ipInfo.wlan0;\n              } else if (ipInfo.eth0) {\n                ipDisplay = ipInfo.eth0;\n              } else if (ipInfo.default) {\n                ipDisplay = ipInfo.default;\n              } else {\n                ipDisplay = "Non connect\xc3\xa9";\n              }\n\n              document.getElementById("ipAddress").textContent = ipDisplay;\n            } else {\n              document.getElementById("ipAddress").textContent =\n                "Non disponible";\n            }\n          })\n          .catch((err) => {\n            console.error("Erreur IP:", err);\n            document.getElementById("ipAddress").textContent = "Erreur";\n          });\n      }\n\n      // Fonction pour afficher la liste des r\xc3\xa9seaux WiFi\n      function displayNetworks(networks) {\n        const currentNetwork = networks.current_network;\n        wifiList.innerHTML = networks.networks\n          .map(\n            (network) => `\n          <div class="wifi-network ${\n            network.ssid === currentNetwork ? "current-network" : ""\n          }" onclick="selectNetwork(\'${network.ssid}\')">\n            <div class="wifi-icon">\n              <i class="fas fa-wifi" style="opacity: ${Math.max(\n                0.3,\n                network.signal / 100\n              )}"></i>\n            </div>\n            <div class="wifi-name">${network.ssid}</div>\n            ${\n              network.ssid === currentNetwork\n                ? \'<div class="connection-status">Connect\xc3\xa9</div>\'\n                : ""\n            }\n          </div>\n        `\n          )\n          .join("");\n      }\n\n      // Fonction pour s\xc3\xa9lectionner et se connecter \xc3\xa0 un r\xc3\xa9seau\n      function selectNetwork(network) {\n        selectedNetwork = network;\n        // V\xc3\xa9rifier les identifiants dans le fichier\n        fetch("/check_wifi_credentials", {\n          method: "POST",\n          headers: {\n            "Content-Type": "application/json",\n          },\n          body: JSON.stringify({\n            ssid: network,\n          }),\n        })\n          .then((response) => response.json())\n          .then((data) => {\n            if (data.hasCredentials) {\n              connectToWifi(data.password);\n            } else {\n              showToast(\n                "Pas d\'identifiants enregistr\xc3\xa9s pour ce r\xc3\xa9seau",\n                "error"\n              );\n            }\n          })\n          .catch((error) => {\n            console.error("Erreur:", error);\n            showToast(\n              "Erreur lors de la v\xc3\xa9rification des identifiants",\n              "error"\n            );\n          });\n      }\n\n      function connectToWifi(password) {\n        const toastMessage = document.querySelector(".toast");\n        toastMessage.textContent = "Tentative de connexion...";\n        toastMessage.className = "toast";\n        toastMessage.style.display = "block";\n\n        fetch("/connect_wifi", {\n          method: "POST",\n          headers: {\n            "Content-Type": "application/json",\n          },\n          body: JSON.stringify({\n            ssid: selectedNetwork,\n            password: password,\n          }),\n        })\n          .then((response) => response.json())\n          .then((data) => {\n            if (data.success) {\n              showToast("Connexion r\xc3\xa9ussie !", "success");\n              setTimeout(() => {\n                location.reload();\n              }, 2000);\n            } else {\n              let errorMsg = "\xc3\x89chec de la connexion";\n              if (data.error) {\n                errorMsg += ": " + data.error;\n              }\n              showToast(errorMsg, "error");\n              console.error("Erreur d\xc3\xa9taill\xc3\xa9e:", data.error);\n            }\n          })\n          .catch((error) => {\n            console.error("Erreur:", error);\n            showToast("Erreur lors de la connexion", "error");\n          });\n      }\n\n      // Fonction pour rafra\xc3\xaechir la liste des r\xc3\xa9seaux\n      function handleRefresh() {\n        const refreshBtn = document.querySelector(".refresh-btn");\n        const toastMessage = document.querySelector(".toast");\n\n        refreshBtn.classList.add("spinning");\n        toastMessage.textContent = "Recherche des r\xc3\xa9seaux...";\n        toastMessage.className = "toast";\n        toastMessage.style.display = "block";\n\n        fetch("/wifi-scan")\n          .then((response) => response.json())\n          .then((data) => {\n            if (data.error) {\n              showToast("Erreur: " + data.error, "error");\n            } else {\n              displayNetworks(data);\n              showToast("R\xc3\xa9seaux mis \xc3\xa0 jour", "success");\n            }\n          })\n          .catch((error) => {\n            console.error("Erreur:", error);\n            showToast("Erreur lors de la recherche des r\xc3\xa9seaux", "error");\n          })\n          .finally(() => {\n            refreshBtn.classList.remove("spinning");\n          });\n      }\n\n      // Initialisation et actualisation p\xc3\xa9riodique\n      handleRefresh();\n      updateConnectionStatus();\n      getIpAddress();\n\n      // Actualiser le statut toutes les 5 secondes\n      setInterval(updateConnectionStatus, 5000);\n      setInterval(getIpAddress, 30000); // Mise \xc3\xa0 jour toutes les 30 secondes\n    </script>\n  </body>\n</html>'
