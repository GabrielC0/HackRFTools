s="control-section">\n        <label class="control-label">\n          <i class="fas fa-radio"></i>\n          Signaux disponibles\n        </label>\n        <div class="file-buttons" id="fileButtons">\n          <!-- Les boutons seront g\xc3\xa9n\xc3\xa9r\xc3\xa9s dynamiquement -->\n        </div>\n      </div>\n    </div>\n\n    <div id="popup"></div>\n\n    <script>\n      let clickCount = 0;\n      let settingsOpen = false;\n      let checkHackRFInterval; // Variable pour stocker l\'intervalle de v\xc3\xa9rification\n\n      // Animation d\'entr\xc3\xa9e\n      gsap.to("#mainContainer", {\n        y: 0,\n        opacity: 1,\n        duration: 1,\n        ease: "power3.out",\n        delay: 0.2,\n      });\n\n      // Cr\xc3\xa9ation des particules\n      function createParticles() {\n        const particlesContainer = document.getElementById("particles");\n        for (let i = 0; i < 50; i++) {\n          const particle = document.createElement("div");\n          particle.className = "particle";\n          particle.style.left = Math.random() * 100 + "%";\n          particle.style.top = Math.random() * 100 + "%";\n          particle.style.animationDelay = Math.random() * 6 + "s";\n          particle.style.animationDuration = Math.random() * 3 + 3 + "s";\n          particlesContainer.appendChild(particle);\n        }\n      }\n\n      // R\xc3\xa9cup\xc3\xa9rer l\'adresse IP\n      function getIpAddress() {\n        fetch("/get-ip")\n          .then((res) => res.json())\n          .then((data) => {\n            if (data.status === "success") {\n              const ipInfo = data.ip_info;\n              let ipDisplay = "";\n\n              // Priorit\xc3\xa9 \xc3\xa0 wlan0, puis eth0, puis default\n              if (ipInfo.wlan0) {\n                ipDisplay = `WiFi: ${ipInfo.wlan0}`;\n              } else if (ipInfo.eth0) {\n                ipDisplay = `Ethernet: ${ipInfo.eth0}`;\n              } else if (ipInfo.default) {\n                ipDisplay = ipInfo.default;\n              } else {\n                ipDisplay = "Non connect\xc3\xa9";\n              }\n\n              document.getElementById("ipAddress").textContent = ipDisplay;\n            } else {\n              document.getElementById("ipAddress").textContent =\n                "Non disponible";\n            }\n          })\n          .catch((err) => {\n            console.error("Erreur lors de la r\xc3\xa9cup\xc3\xa9ration de l\'IP:", err);\n            document.getElementById("ipAddress").textContent = "Erreur";\n          });\n      }\n\n      // Gestion du menu param\xc3\xa8tres\n      function toggleSettings() {\n        const menu = document.getElementById("settingsMenu");\n        settingsOpen = !settingsOpen;\n\n        if (settingsOpen) {\n          menu.classList.add("active");\n          // Simuler la v\xc3\xa9rification du statut HackRF\n          checkHackRFStatus();\n        } else {\n          menu.classList.remove("active");\n        }\n      }\n\n      // Fermer le menu si on clique ailleurs\n      document.addEventListener("click", function (event) {\n        const settingsButton = document.querySelector(".settings-button");\n        const settingsMenu = document.getElementById("settingsMenu");\n\n        if (\n          !settingsButton.contains(event.target) &&\n          !settingsMenu.contains(event.target)\n        ) {\n          settingsMenu.classList.remove("active");\n          settingsOpen = false;\n        }\n      });\n\n      // V\xc3\xa9rification du statut HackRF\n      function checkHackRFStatus() {\n        const mainStatusElement = document.getElementById(\n          "mainConnectionStatus"\n        );\n\n        // Ne pas montrer d\'\xc3\xa9tat interm\xc3\xa9diaire de v\xc3\xa9rification\n        fetch("/hackrf-status")\n          .then((res) => res.json())\n          .then((data) => {\n            if (data.status === "connected") {\n              // Afficher seulement l\'\xc3\xa9tat connect\xc3\xa9\n              mainStatusElement.innerHTML =\n                \'<span class="status-connected"><i class="fas fa-signal"></i> HackRF connect\xc3\xa9</span>\';\n\n              // Stocker l\'horodatage de la derni\xc3\xa8re v\xc3\xa9rification r\xc3\xa9ussie\n              const lastConnectTime = new Date();\n              mainStatusElement.setAttribute(\n                "data-last-connect",\n                lastConnectTime.getTime()\n              );\n            } else {\n              // Afficher seulement l\'\xc3\xa9tat d\xc3\xa9connect\xc3\xa9\n              mainStatusElement.innerHTML =\n                \'<span class="status-disconnected"><i class="fas fa-exclamation-triangle"></i> HackRF non connect\xc3\xa9</span>\';\n            }\n          })\n          .catch(() => {\n            // En cas d\'erreur, afficher comme d\xc3\xa9connect\xc3\xa9\n            mainStatusElement.innerHTML =\n              \'<span class="status-disconnected"><i class="fas fa-exclamation-circle"></i> HackRF non connect\xc3\xa9</span>\';\n          });\n      }\n\n      // Mise \xc3\xa0 jour du compteur\n      function updateClickCounter() {\n        fetch("/click-count")\n          .then((res) => res.json())\n          .then((data) => {\n            clickCount = data.count;\n            document.getElementById("clickCounter").textContent = clickCount;\n          })\n          .catch((err) => {\n            console.error("Erreur lors de la mise \xc3\xa0 jour du compteur:", err);\n          });\n      }\n\n      // Popup de confirmation\n      function showConfirmation(type) {\n        const overlay = document.getElementById("confirmationOverlay");\n        const popup = document.getElementById("confirmationPopup");\n        const icon = document.getElementById("confirmationIcon");\n        const title = document.getElementById("confirmationTitle");\n        const message = document.getElementById("confirmationMessage");\n        const confirmBtn = document.getElementById("confirmBtn");\n\n        if (type === "reset") {\n          icon.innerHTML = "\xf0\x9f\x94\x84";\n          title.textContent = "Confirmer le Reset";\n          message.textContent =\n            "\xc3\x8ates-vous s\xc3\xbbr de vouloir remettre le compteur \xc3\xa0 z\xc3\xa9ro ?";\n          confirmBtn.onclick = () => {\n            resetCounter();\n            hideConfirmation();\n          };\n        } else if (type === "shutdown") {\n          icon.innerHTML = "\xe2\x9a\xa0\xef\xb8\x8f";\n          title.textContent = "Confirmer l\'arr\xc3\xaat";\n          message.textContent =\n            "\xc3\x8ates-vous s\xc3\xbbr de vouloir \xc3\xa9teindre le syst\xc3\xa8me ? Cette action ne peut pas \xc3\xaatre annul\xc3\xa9e.";\n          confirmBtn.onclick = () => {\n            shutdown();\n            hideConfirmation();\n          };\n        }\n\n        overlay.style.display = "flex";\n        setTimeout(() => {\n          popup.classList.add("active");\n        }, 10);\n      }\n\n      function hideConfirmation() {\n        const overlay = document.getElementById("confirmationOverlay");\n        const popup = document.getElementById("confirmationPopup");\n\n        popup.classList.remove("active");\n        setTimeout(() => {\n          overlay.style.display = "none";\n        }, 300);\n      }\n\n      // Actions des boutons\n      function resetCounter() {\n        const resetBtn = document.querySelector(".reset-btn");\n        const originalText = resetBtn.innerHTML;\n        resetBtn.innerHTML =\n          \'<span class="hourglass" style="display:inline-block;width:1.2em;height:1.2em;">\xe2\x8f\xb3</span> Reset\';\n        resetBtn.disabled = true;\n        fetch("/reset-count", { method: "POST" })\n          .then((res) => res.json())\n          .then((data) => {\n            clickCount = 0;\n            document.getElementById("clickCounter").textContent = clickCount;\n            showPopup(\n              data.message || "Compteur remis \xc3\xa0 z\xc3\xa9ro",\n              data.status === "success"\n            );\n          })\n          .catch(() => {\n            showPopup("Erreur lors du reset", false);\n          })\n          .finally(() => {\n            resetBtn.innerHTML = originalText;\n            resetBtn.disabled = false;\n          });\n      }\n\n      function shutdown() {\n        const shutdownBtn = document.querySelector(".shutdown-btn");\n        const originalText = shutdownBtn.innerHTML;\n        shutdownBtn.innerHTML =\n          \'<span class="hourglass" style="display:inline-block;width:1.2em;height:1.2em;">\xe2\x8f\xb3</span> \xc3\x89teindre\';\n        shutdownBtn.disabled = true;\n        fetch("/shutdown", { method: "POST" })\n          .then((res) => res.json())\n          .then((data) => {\n            showPopup(\n              data.message || "Arr\xc3\xaat du syst\xc3\xa8me en cours...",\n              data.status === "success"\n            );\n          })\n          .catch(() => {\n            showPopup("Erreur lors de l\'arr\xc3\xaat", false);\n          })\n          .finally(() => {\n            shutdownBtn.innerHTML = originalText;\n            shutdownBtn.disabled = false;\n          });\n      }\n\n      const popup = document.getElementById("popup");\n\n      function showPopup(msg, success = true) {\n        // Pr\xc3\xa9fixer avec l\'ic\xc3\xb4ne appropri\xc3\xa9e\n        const iconHTML = success\n          ? `<i class="fas fa-check-circle"></i> `\n          : `<i class="fas fa-exclamation-triangle"></i> `;\n\n        // Le message peut contenir du HTML (pour les d\xc3\xa9tails)\n        popup.innerHTML = iconHTML + msg;\n\n        popup.className = success ? "" : "popup-error";\n        popup.style.display = "block";\n\n        gsap.fromTo(\n          popup,\n          { y: 100, opacity: 0, scale: 0.8 },\n          { y: 0, opacity: 1, scale: 1, duration: 0.5, ease: "back.out(1.7)" }\n        );\n\n        // Dur\xc3\xa9e plus longue si le message contient des d\xc3\xa9tails\n        const displayDuration = msg.includes("<br>") ? 5000 : 3500;\n\n        setTimeout(() => {\n          gsap.to(popup, {\n            y: 100,\n            opacity: 0,\n            scale: 0.8,\n            duration: 0.4,\n            ease: "back.in(1.7)",\n            onComplete: () => (popup.style.display = "none"),\n          });\n        }, displayDuration);\n      }\n\n      function showLoading(button) {\n        const originalText = button.innerHTML;\n        button.innerHTML =\n          \'<span class="hourglass" style="display:inline-block;width:1.2em;height:1.2em;">\xe2\x8f\xb3</span>\';\n        button.disabled = true;\n        return originalText;\n      }\n\n      function hideLoading(button, originalText) {\n        button.innerHTML = originalText;\n        button.disabled = false;\n      }\n\n      function loadFiles() {\n        const folder = document.getElementById("folder").value;\n        const container = document.getElementById("fileButtons");\n\n        // Animation de sortie des anciens boutons\n        gsap.to(container.children, {\n          scale: 0,\n          opacity: 0,\n          duration: 0.3,\n          stagger: 0.05,\n          onComplete: () => {\n            fetch(`/files?folder=${folder}`)\n              .then((res) => res.json())\n              .then((data) => {\n                container.innerHTML = "";\n                data.forEach((file, index) => {\n                  const letter = file.split(".")[0];\n                  const btn = document.createElement("button");\n                  btn.textContent = letter;\n                  btn.className = "btn";\n                  btn.onclick = () => sendFile(folder, letter, btn);\n                  btn.style.opacity = "0";\n                  btn.style.transform = "scale(0.8)";\n                  container.appendChild(btn);\n\n                  // Animation d\'entr\xc3\xa9e des nouveaux boutons\n                  gsap.to(btn, {\n                    opacity: 1,\n                    scale: 1,\n                    duration: 0.4,\n                    delay: index * 0.1,\n                    ease: "back.out(1.7)",\n                  });\n                });\n              })\n              .catch((err) => {\n                showPopup("Erreur lors du chargement des fichiers", false);\n              });\n          },\n        });\n      }\n\n      function sendFile(folder, letter, button) {\n        const originalText = showLoading(button);\n\n        // V\xc3\xa9rifier d\'abord si le HackRF est connect\xc3\xa9\n        fetch("/hackrf-status")\n          .then((res) => res.json())\n          .then((data) => {\n            if (data.status === "connected") {\n              // Si le HackRF est connect\xc3\xa9, proc\xc3\xa9der \xc3\xa0 l\'envoi\n              fetch(`/send?folder=${folder}&letter=${letter}`)\n                .then((res) => res.json())\n                .then((data) => {\n                  hideLoading(button, originalText);\n\n                  // Simplifier le message de confirmation d\'envoi\n                  if (data.status === "success") {\n                    showPopup("\xe2\x9c\x85 Signal envoy\xc3\xa9 avec succ\xc3\xa8s", true);\n                    updateClickCounter();\n\n                    // Animation de succ\xc3\xa8s sur le bouton\n                    gsap.to(button, {\n                      scale: 1.1,\n                      duration: 0.2,\n                      yoyo: true,\n                      repeat: 1,\n                      ease: "power2.inOut",\n                    });\n                  } else {\n                    showPopup(data.message || "Erreur lors de l\'envoi", false);\n                  }\n                })\n                .catch((err) => {\n                  hideLoading(button, originalText);\n                  showPopup("Erreur lors de l\'envoi", false);\n                });\n            } else {\n              // Si le HackRF n\'est pas connect\xc3\xa9, afficher une erreur\n              hideLoading(button, originalText);\n              showPopup(\n                "HackRF non d\xc3\xa9tect\xc3\xa9. Veuillez v\xc3\xa9rifier la connexion.",\n                false\n              );\n            }\n          })\n          .catch((err) => {\n            hideLoading(button, originalText);\n            showPopup("Erreur lors de la v\xc3\xa9rification du HackRF", false);\n          });\n      }\n\n      // Initialisation\n      createParticles();\n      loadFiles();\n      loadClickCount();\n      checkHackRFStatus(); // V\xc3\xa9rifier le statut HackRF au d\xc3\xa9marrage\n      startPeriodicHackRFCheck(); // D\xc3\xa9marrer la v\xc3\xa9rification p\xc3\xa9riodique\n      getIpAddress(); // R\xc3\xa9cup\xc3\xa9rer l\'adresse IP\n\n      // Rafra\xc3\xaechir r\xc3\xa9guli\xc3\xa8rement l\'adresse IP\n      setInterval(() => {\n        getIpAddress();\n      }, 30000); // Toutes les 30 secondes\n\n      // Fonction pour d\xc3\xa9marrer la v\xc3\xa9rification p\xc3\xa9riodique du HackRF\n      function startPeriodicHackRFCheck() {\n        // Nettoyer l\'ancien intervalle s\'il existe\n        if (checkHackRFInterval) {\n          clearInterval(checkHackRFInterval);\n        }\n\n        // Cr\xc3\xa9er un nouvel intervalle qui v\xc3\xa9rifie toutes les 3 secondes\n        checkHackRFInterval = setInterval(function () {\n          checkHackRFStatus();\n        }, 3000); // 3000 ms = 3 secondes\n      }\n\n      // Chargement du compteur au d\xc3\xa9marrage\n      function loadClickCount() {\n        fetch("/click-count")\n          .then((res) => res.json())\n          .then((data) => {\n            clickCount = data.count;\n            document.getElementById("clickCounter").textContent = clickCount;\n          })\n          .catch((err) => {\n            console.error("Erreur lors du chargement du compteur:", err);\n          });\n      }\n\n      // Animation continue du gradient\n      gsap.to("body", {\n        backgroundPosition: "200% 50%",\n        duration: 20,\n        ease: "none",\n        repeat: -1,\n      });\n\n      // Arr\xc3\xaater la v\xc3\xa9rification p\xc3\xa9riodique lorsque l\'utilisateur quitte la page\n      window.addEventListener("beforeunload", function () {\n        if (checkHackRFInterval) {\n          clearInterval(checkHackRFInterval);\n        }\n      });\n    </script>\n    <script src="/static/touch-animations.js"></script>\n  </body>\n</html>'
